{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "rules": {
        "no-console": 1, // Means warning
        "@typescript-eslint/no-unused-vars": 2, // 2 means error
        "@typescript-eslint/no-var-requires": 1 //override this to warning
        // "prettier/prettier": 2 // Means error
    }
}